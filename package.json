{
  "name": "tor-hazae41",
  "version": "0.1.0",
  "description": "A TypeScript Tor client library for making anonymous HTTP requests through the Tor network via Snowflake bridges",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "tor",
    "proxy",
    "anonymous",
    "snowflake",
    "privacy",
    "onion",
    "client",
    "typescript"
  ],
  "author": "Andrew Morris",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/voltrevo/tor-hazae41.git"
  },
  "scripts": {
    "build": "tsup",
    "build:demo": "vite build",
    "dev": "vite",
    "dev:lib": "tsup --watch",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@brumewallet/wallet.wasm": "^1.0.2",
    "@hazae41/binary": "^1.3.5",
    "@hazae41/cadenas": "^0.4.2",
    "@hazae41/cascade": "^2.2.2",
    "@hazae41/chacha20poly1305": "^1.0.8",
    "@hazae41/echalote": "^0.4.18",
    "@hazae41/ed25519": "^2.1.22",
    "@hazae41/fleche": "^1.4.5",
    "@hazae41/keccak256": "^1.0.10",
    "@hazae41/ripemd160": "^1.0.3",
    "@hazae41/sha1": "^1.1.14",
    "@hazae41/x25519": "^2.2.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^22.0.0",
    "buffer": "^6.0.3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "tslib": "^2.8.1",
    "tsup": "^8.3.5",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.10"
  }
}
