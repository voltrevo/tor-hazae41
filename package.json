{
  "name": "tor-hazae41",
  "version": "0.1.1",
  "description": "A TypeScript Tor client library for making anonymous HTTP requests through the Tor network via Snowflake bridges",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "tor",
    "proxy",
    "anonymous",
    "snowflake",
    "privacy",
    "onion",
    "client",
    "typescript"
  ],
  "author": "Andrew Morris",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/voltrevo/tor-hazae41.git"
  },
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly --declarationMap --outDir dist",
    "build:demo": "vite build",
    "dev": "vite",
    "dev:lib": "tsup --watch",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@brumewallet/wallet.wasm": "1.0.2",
    "@hazae41/asn1": "1.3.31",
    "@hazae41/base16": "1.0.18",
    "@hazae41/base64": "1.0.15",
    "@hazae41/base64url": "1.0.17",
    "@hazae41/binary": "1.3.5",
    "@hazae41/box": "2.0.1",
    "@hazae41/bytes": "1.2.11",
    "@hazae41/cadenas": "0.4.2",
    "@hazae41/cascade": "2.2.2",
    "@hazae41/chacha20poly1305": "1.0.8",
    "@hazae41/cursor": "1.2.4",
    "@hazae41/disposer": "2.0.20",
    "@hazae41/echalote": "0.4.18",
    "@hazae41/ed25519": "2.1.22",
    "@hazae41/fleche": "1.4.5",
    "@hazae41/future": "1.0.3",
    "@hazae41/kcp": "1.1.3",
    "@hazae41/keccak256": "1.0.10",
    "@hazae41/mutex": "2.1.0",
    "@hazae41/option": "1.1.4",
    "@hazae41/plume": "3.0.5",
    "@hazae41/result": "1.3.2",
    "@hazae41/ripemd160": "1.0.3",
    "@hazae41/sha1": "1.1.14",
    "@hazae41/signals": "1.0.2",
    "@hazae41/smux": "1.1.3",
    "@hazae41/symbol-dispose-polyfill": "1.0.2",
    "@hazae41/x25519": "2.2.9",
    "@hazae41/x509": "1.2.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^22.0.0",
    "buffer": "^6.0.3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "tslib": "^2.8.1",
    "tsup": "^8.3.5",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.10"
  }
}
